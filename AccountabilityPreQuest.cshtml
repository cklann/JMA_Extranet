@{
    if (WebSecurity.IsAuthenticated) {

        var db=Database.Open("JMA");
        var SQLSELECT="select * from Accountability where Email LIKE @0";
        var answers=db.QuerySingle(SQLSELECT, WebSecurity.CurrentUserName);

        var ansQ1=answers.Q1;
        var ansQ2=answers.Q2;
        var ansQ3=answers.Q3;
        var ansQ4=answers.Q4;
        var ansQ5=answers.Q5;
        var ansQ6=answers.Q6;
        var ansQ7=answers.Q7;
        var ansQ8=answers.Q8;
        var ansQ9=answers.Q9;
        var ansQ10=answers.Q10;

        if (IsPost) {
            ansQ1=Request["ansQ1"].As<string>("");
            ansQ2=Request["ansQ2"].As<string>("");
            ansQ3=Request["ansQ3"].As<string>("");
            ansQ4=Request["ansQ4"].As<string>("");
            ansQ5=Request["ansQ5"].As<string>("");
            ansQ6=Request["ansQ6"].As<string>("");
            ansQ7=Request["ansQ7"].As<string>("");
            ansQ8=Request["ansQ8"].As<string>("");
            ansQ9=Request["ansQ9"].As<string>("");
            ansQ10=Request["ansQ10"].As<string>("");

            var SQLUPDATE="update Accountability set Q1=@1, Q2=@2, Q3=@3, Q4=@4, Q5=@5, " +
                "Q6=@6, Q7=@7, Q8=@8, Q9=@9, Q10=@10 " +
                "where Email=@0";
            db.Execute(SQLUPDATE, WebSecurity.CurrentUserName, ansQ1, ansQ2, ansQ3, ansQ4,
                ansQ5, ansQ6, ansQ7, ansQ8, ansQ9, ansQ10);
            if (Request["submit"]=="Return Home") {
                Response.Redirect("~/index.cshtml");
            }
        }

<form action="" method="post">
